<div class="row">
  <div class="col-md-8">
     
    <div class="page-header">
        <h2>Listing Weekends</h2>
    </div>
       

  </div>

    <div class="col-md-4">
      <div class="page-header">
        <h2>Adding Weekends</h2>
      </div>
       <div id="errorMsg">
         
       </div>
        <%= render "form" %>
       
   </div>

</div>
<h1>Listing Weekends</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Claim</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weekends.each do |weekend| %>
      <tr>
        <td><%= weekend.date %></td>
        <td><%= weekend.claim %></td>
        <td><%= link_to 'Show', weekend %></td>
        <td><%= link_to 'Edit', edit_weekend_path(weekend) %></td>
        <td><%= link_to 'Destroy', weekend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weekend', new_weekend_path %>
